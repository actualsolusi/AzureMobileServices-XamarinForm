<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mugiservices.MahasiswaPage">
  <StackLayout>
    <ListView ItemsSource="{Binding ListItems}" x:Name="myList">
      <ListView.ItemTemplate>
        <DataTemplate>
          <TextCell Text="{Binding Nim}" Detail="{Binding Nama}" DetailColor="Red" />
        </DataTemplate>
      </ListView.ItemTemplate>
    </ListView>
  </StackLayout>

</ContentPage>